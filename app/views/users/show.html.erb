<h1>My Profile</h1>
<div class="row">
  <h4>My Blocmarks</h4>
    <div class="col-md-8">
        <% @user_bookmarks.each do |bookmark| %>
          <ul>
              <%="#{bookmark.topic.title}"%>
            <li><%= link_to bookmark.url %></li>
          </ul>
        <% end %>
    </div>
</div>
<div class="row">
  <h4>Liked Blocmarks</h4>
    <div class="col-md-8">
      <% @liked_bookmarks.each do |bookmark| %>
        <ul>
          <%="#{bookmark.topic.title}"%>
          <li>
            <%= link_to bookmark.url %>
            <%= render partial: 'likes/like', locals: { bookmark: bookmark } %>
          </li>
        </ul>
      <% end %>
    </div>
</div>
