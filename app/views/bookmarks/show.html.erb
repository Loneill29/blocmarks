<h1 class="page-title"> <%= @bookmark.url %> </h1>
  <div class="page-title">
  <div class="edit-delete">
    <%= link_to "Delete Bookmark", [@bookmark.topic, @bookmark], method: :delete, class: 'btn btn-danger' %>
    <%= link_to "Edit Bookmark", edit_topic_bookmark_path(@bookmark.topic, @bookmark), class: 'btn btn-success', id: "edit" %>
  </div>
  <div class="likes">
    <%= render partial: 'likes/like', locals: { bookmark: @bookmark } %>
  </div>
  </div>
